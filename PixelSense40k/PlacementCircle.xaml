<s:TagVisualization x:Class="PixelSense40k.PlacementCircle"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:s="http://schemas.microsoft.com/surface/2008"
    Loaded="PlacementCircle_Loaded">
    <Grid>
        <Ellipse Height="150.0" Width="150.0" Fill="CadetBlue"/>
        <Rectangle Name="iBoundary" Height="75" Width="75" />
        <Label Content="Put it back" FontSize="8" VerticalAlignment="Center" HorizontalAlignment="Center"/>
        <s:SurfaceButton Content="Lock" FontSize="10" HorizontalAlignment="Right" VerticalAlignment="Bottom" Click="lockUnlock"/>
    </Grid>
</s:TagVisualization>
